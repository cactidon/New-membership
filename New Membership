<script>
function myFunction() {

	var member = document.forms.form1;
	var txt = "";
	var years = document.getElementById("mem_years").value;
	var i;
	for (i = 0; i < member.length; i++) {
		if (member[i].checked) {
			txt = txt + member[i].value + "#";}
	}

	var txt2 = "";
	if(txt.substring(0,4) == "yes#") {txt2 = txt.substring(4);} else {txt2 = txt.substring(3); }

	if (txt2 == "elect#") {total = years * 10;} else {total = years * 20;}
	document.getElementById("mem_amount").value = total.toFixed(2);
 
	var num2 = document.getElementById("num_members").value;			
	total_members = num2 * 10;
	document.getElementById("amount_members").value = total_members.toFixed(2);
	
	var num3 = document.getElementById("num_guests").value;			
	total_guests = num3 * 5;
	document.getElementById("amount_guests").value = total_guests.toFixed(2);
	
	var num4 = document.getElementById("num_meal1").value;			
	total_meal1 = num4 * 65;
	document.getElementById("amount_meal1").value = total_meal1.toFixed(2);
	
	var num5 = document.getElementById("num_meal2").value;			
	total_meal2 = num5 * 65;
	document.getElementById("amount_meal2").value = total_meal2.toFixed(2);

	var num8 = document.getElementById("hospitality").value;
	total_hospitality = num8 * 1;
	document.getElementById("hospitality").value = total_hospitality.toFixed(2);	
	
	var num9 = document.getElementById("health").value;
	total_health = num9 * 1;
	document.getElementById("health").value = total_health.toFixed(2);
	
	var num10 = document.getElementById("times").value;
	total_times = num10 * 1;
	document.getElementById("times").value = total_times.toFixed(2);
	
	var num11 = document.getElementById("scholarship").value;
	total_scholarship = num11 * 1;
	document.getElementById("scholarship").value = total_scholarship.toFixed(2);
	
	var num12 = document.getElementById("actives").value;
	total_actives = num12 * 1;
	document.getElementById("actives").value = total_actives.toFixed(2);
	
	var num13 = document.getElementById("reserve").value;
	total_reserve = num13 * 1;
	document.getElementById("reserve").value = total_reserve.toFixed(2);
	
	var num14 = document.getElementById("memoriam").value;
	total_memoriam = num14 * 1;
	document.getElementById("memoriam").value = total_memoriam.toFixed(2);
	
	
	var grand_total = 0;
	grand_total = total + total_members + total_guests + total_meal1 + total_meal2  + total_hospitality + total_health +
	total_times + total_scholarship + total_actives +
					total_reserve + total_memoriam;					
	document.getElementById("grand_total").value = grand_total.toFixed(2);
	
	if (grand_total > 0) {
		document.getElementById("submitok").disabled = false;	
	} else {
		document.getElementById("submitok").disabled = true;	
	}
}
</script>
	

			
</body></html>
